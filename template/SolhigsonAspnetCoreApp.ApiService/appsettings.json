{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Error",
      "Hangfire": "Error"
    }
  },
  "AllowedHosts": "*",
  "Initialization": {
    "LogLevel": "info",
    "ProtectedFields": "password,pin",
    "HangfireDashboardAuth": "password"
  },
  "Jwt": {
    "ExpirationHrs": "2"
  },
  "AppSettings": {
    "UserTokenValidityPeriodHrs": "2"
  }
}